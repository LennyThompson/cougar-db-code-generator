{
  "assembly-names": [
    "RegulatorReportingDB",
    "GamingMetersDB.RegReport",
    "GamingSettingsDB.RegReport",
    "OLGRReports.Grammar"
  ],
  "ef-contexts": [
    {
      "type-namespace": "RegulatorReportingDB.model.views",
      "context": "RegulatorreportingContext",
      "context-namespace": "RegulatorReportingDB.model.context.views",
      "context-source": "RegulatorReportingViews",
      "context-service": "DataContextViewModel",
      "context-service-namespace": "CougarTestHarness.Models"
    },
    {
      "type-namespace": "CougarDb.Models.Cougar.Tables",
      "context": "CougarContext",
      "context-namespace": "CougarDb.Models.Context.Tables",
      "context-source": "CougarDbTables",
      "context-service": "DataContextViewModel",
      "context-service-namespace": "CougarTestHarness.Models"
    },
    {
      "type-namespace": "GamingMetersDB.RegReport.model.tables",
      "context": "GamingmetersContext",
      "context-namespace": "GamingMetersDB.RegReport.model.context.tables",
      "context-source": "GamingMetersTables",
      "context-service": "DataContextViewModel",
      "context-service-namespace": "CougarTestHarness.Models"
    }
  ],
  "templates": [
    {
      "template": "generateDartDataGridType",
      "file-extesion": "data_grid.dart",
      "snake-case-filename": true,
      "out-dir": "views",
      "target": "Each",
      "generate-type": "dart",
      "output-language": "dart"
    },
    {
      "template": "generateDartType",
      "file-extesion": "dart",
      "snake-case-filename": true,
      "out-dir": "model",
      "target": "Each",
      "generate-type": "dart",
      "output-language": "dart"
    },
    {
      "template": "generateSingleInstanceFlutterTable",
      "file-extesion": "view.dart",
      "snake-case-filename": true,
      "filename-getter": "DetailViewName",
      "out-dir": "views/single",
      "target": "Each",
      "generate-type": "dart-single-value-table",
      "output-language": "dart"
    },
    {
      "template": "generateSingleInstanceStateProvider",
      "file-extesion": "state.dart",
      "snake-case-filename": true,
      "filename-getter": "DetailStateProviderName",
      "out-dir": "state",
      "target": "Each",
      "generate-type": "dart-single-value-table",
      "output-language": "dart"
    },
    {
      "template": "generateDataProviderType",
      "file-extesion": "service.dart",
      "snake-case-filename": true,
      "out-dir": "service",
      "target": "Each",
      "generate-type": "dart",
      "output-language": "dart"
    },
    {
      "template": "generateDartApiFutureProvider",
      "file-extesion": "provider.dart",
      "snake-case-filename": true,
      "out-dir": "providers",
      "target": "Each",
      "generate-type": "dart",
      "output-language": "dart"
    },
    {
      "template": "generateDartStateProvider",
      "file-extesion": "state.dart",
      "snake-case-filename": true,
      "out-dir": "state",
      "target": "Each",
      "generate-type": "dart",
      "output-language": "dart"
    },
    {
      "template": "generateDartFilterType",
      "file-extesion": "filter.dart",
      "snake-case-filename": true,
      "out-dir": "filter",
      "target": "Each",
      "generate-type": "dart",
      "output-language": "dart"
    },
    {
      "template": "generateDartFilterStateProvider",
      "file-extesion": "filter.state.dart",
      "snake-case-filename": true,
      "out-dir": "state/filter",
      "target": "Each",
      "generate-type": "dart"
    },
    {
      "template": "generateDartFilterLibraryExports",
      "file-extesion": "cougar_db_filter.dart",
      "snake-case-filename": true,
      "out-dir": "filter-lib",
      "target": "All",
      "generate-type": "dart",
      "output-language": "dart"
    },
    {
      "template": "generateReportViewModel",
      "file-extesion": "ViewModel.cs",
      "out-dir": "RegulatorReports",
      "target": "Each",
      "generate-type": "view-model"
    },
    {
      "template": "generateTableFilterModel",
      "file-extesion": "Filter.cs",
      "out-dir": "ViewModel-Filter",
      "target": "Each",
      "generate-type": "view-model-filter"
    },
    {
      "template": "generateWebApiController",
      "file-extesion": "Controller.cs",
      "out-dir": "WebApi",
      "target": "Each",
      "generate-type": "web-api"
    },
    {
      "template": "generateDataFillerViewModel",
      "file-extesion": "DataFillerViewModel.cs",
      "out-dir": "DataFiller",
      "target": "All",
      "generate-type": "view-model-filler"
    },
    {
      "template": "generateReportManagerServices",
      "file-extesion": "ReportingManager.part.cs",
      "out-dir": "Services",
      "target": "All",
      "generate-type": "view-model"
    },
    {
      "template": "generateReportFlutterPage",
      "file-extesion": "RegulatorReportingHomePage.dart",
      "out-dir": "views",
      "target": "All",
      "generate-type": "dart-report",
      "output-language": "dart"
    },
    {
      "template": "generateDartEnumType",
      "file-extesion": "enum.dart",
      "snake-case-filename": true,
      "out-dir": "model",
      "target": "Each",
      "generate-type": "dart-enum",
      "output-language": "dart"
    },
    {
      "template": "generateCougarMessagePackageDart",
      "file-extesion": "cougar-messages.dart",
      "snake-case-filename": true,
      "out-dir": "model",
      "target": "All",
      "generate-type": "dart-package",
      "output-language": "dart",
      "generate-depends": [
        "dart",
        "dart-enum"
      ]
    },
    {
      "template": "generateCougarMessageFactoryDart",
      "file-extesion": "cougar-message-factory.dart",
      "snake-case-filename": true,
      "out-dir": "model",
      "target": "All",
      "generate-type": "dart-message-factory",
      "output-language": "dart",
      "generate-depends": [
        "dart"
      ]
    },
    {
      "template": "generateCougarCougarDbController",
      "file-extesion": "api.cs",
      "snake-case-filename": false,
      "out-dir": "model/cougar-db/api",
      "target": "Each",
      "generate-type": "cougar-db-web-api",
      "generate-depends": []

    },
    {
      "template": "generateCougarCougarDbViewModel",
      "file-extesion": "viewmodel.cs",
      "snake-case-filename": false,
      "out-dir": "model/cougar-db/viewmodel",
      "target": "Each",
      "generate-type": "cougar-db-web-api",
      "generate-depends": []

    }

  ],
  "template-root": "./Templates",
  "output-root": "./generated",
  "meta-data":  "./Scripts/cougar-db-meta-data.json",
  "generate-assemblies": [
    {
      "name": "Maxgaming.Cougar.Message.Net6",
      "ignore":  true,
      "namespaces": [
        "Maxgaming.Cougar.Message"
      ],
      "generate-reflected": [
        {
          "type": "enum",
          "generate-types": [
            {
              "name": "dart-enum",
              "folder": "messages"
            },
            {
              "name": "dart-package",
              "folder": "messages"
            }
          ]
        },
        {
          "type": "class",
          "generate-types": [
            {
              "name": "dart",
              "folder": "messages"
            },
            {
              "name": "dart-report",
              "folder": "messages"
            },
            {
              "name": "dart-package"
            },
            {
              "name": "dart-message-factory"
            }
          ],
          "generate-types-factory": [
            {
              "name": "dart-message-factory"
            }
          ],
          "requires-base-type": [
            "CougarMessage"
          ],
          "ignore-generic": true,
          "supress-fields": [
            {
              "name": "DynamicMessageExtraData",
              "compare": "all"
            },
            {
              "name": "WithExtraData",
              "compare": "ends"
            }
          ],
          "inject-fields": [
            {
              "name": "CougarMessage",
              "source": "GetPostOfficeFunction"
            }
          ]
        }
      ]
    },
    {
      "name": "cougar-db-efcore",
      "ignore": false,
      "namespaces": [
        "CougarDb.Models.Cougar.Tables",
        "CougarDb.Models.Cougar.Views"
      ],
      "generate-reflected": [
        {
          "type": "enum",
          "generate-types": [
            {
              "name": "dart-enum",
              "folder": "cougar-db"
            },
            {
              "name": "dart-package"
            }
          ]
        },
        {
          "type": "class",
          "generate-types": [
            {
              "name": "dart",
              "folder": "cougar-db"
            },
            {
              "name": "dart-report",
              "folder": "cougar-db"
            },
            {
              "name": "dart-package"
            },
            {
              "name": "cougar-db-web-api"
            },
            {
              "name": "view-model-filter"
            },
            {
              "name": "dart-single-value-table"
            }
          ],
          "ignore-generic": true,
          "supress-fields": [],
          "inject-fields": []
        }
      ]
    }
  ]
}